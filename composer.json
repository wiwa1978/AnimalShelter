{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "amendozaaguiar/filament-route-statistics": "^3.0",
        "aymanalhattami/filament-context-menu": "^1.0",
        "bezhansalleh/filament-shield": "^3.2",
        "bilfeldt/laravel-route-statistics": "^3.4",
        "brickx/maintenance-switch": "^1.0",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "flowframe/laravel-trend": "^0.2.0",
        "howdu/filament-record-switcher": "^1.0",
        "joaopaulolndev/filament-check-ssl-widget": "^1.0",
        "joaopaulolndev/filament-edit-env": "^1.0",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "joaopaulolndev/filament-general-settings": "^1.0",
        "kenepa/banner": "^0.0.5",
        "laravel/framework": "^11.0",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "^1.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "livewire/livewire": "^3.4",
        "mansoor/filament-versionable": "^0.0.8",
        "marcogermani87/filament-cookie-consent": "^1.0",
        "njxqlus/filament-progressbar": "^1.0",
        "rappasoft/laravel-authentication-log": "^4.0",
        "rmsramos/activitylog": "^1.0",
        "saade/filament-laravel-log": "^3.2",
        "schmeits/filament-character-counter": "^1.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.7",
        "tapp/filament-authentication-log": "^3.1",
        "tapp/filament-maillog": "^1.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "smknstd/fakerphp-picsum-images": "^1.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://spark.laravel.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
